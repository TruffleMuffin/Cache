<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="castle" type="Castle.Windsor.Configuration.AppDomain.CastleSectionHandler, Castle.Windsor" />
  </configSections>
  <castle>
    <installers>
      <install type="TruffleCache.Example.Installer, TruffleCache.Example" />
    </installers>
  </castle>
  <appSettings>
    <add key="TruffleCache.CachePrefix" value="Example_"/>
    <add key="TruffleCache.ExpiryInHours" value="1"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>